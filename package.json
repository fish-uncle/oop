{
	"name": "oop",
	"version": "0.0.1",
	"keywords": [
		"typescript",
		"vue",
		"vuejs",
		"pug",
		"oop",
		"scss",
		"element-plus",
		"vue3"
	],
	"scripts": {
		"dev": "vite --mode development",
		"build": "vue-tsc --noEmit && vite build --mode production",
		"preview": "vite preview",
		"commit": "git add .&&git-cz"
	},
	"dependencies": {
		"@vue/compiler-sfc": "^3.2.25",
		"axios": "^0.21.2",
		"core-js": "^3.6.5",
		"element-plus": "^2.1.11",
		"md5": "^2.3.0",
		"shortid": "^2.2.16",
		"vue": "^3.2.25",
		"vue-loader": "^16.5.0",
		"vue-router": "^4.0.11",
		"vuedraggable": "^4.0.3"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.13.5",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
		"@babel/plugin-proposal-optional-chaining": "^7.12.1",
		"@babel/plugin-transform-classes": "^7.14.2",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@commitlint/cli": "^9.1.2",
		"@commitlint/config-conventional": "^13.1.0",
		"@prettier/plugin-pug": "^1.16.5",
		"@types/shortid": "0.0.29",
		"@typescript-eslint/eslint-plugin": "^4.20.0",
		"@typescript-eslint/parser": "^4.20.0",
		"@vitejs/plugin-legacy": "^1.8.1",
		"@vitejs/plugin-vue": "^2.3.1",
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-plugin-typescript": "^4.5.8",
		"@vue/eslint-config-prettier": "^6.0.0",
		"@vue/eslint-config-standard": "^5.1.2",
		"@vue/eslint-config-typescript": "^7.0.0",
		"autoprefixer": "^10.4.4",
		"babel-eslint": "^8.1.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"commitizen": "^4.2.3",
		"commitlint-config-cz": "^0.13.2",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^6.7.2",
		"eslint-plugin-flowtype": "^5.4.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-sonarjs": "^0.6.0",
		"eslint-plugin-standard": "^5.0.0",
		"eslint-plugin-vue": "^6.2.2",
		"git-cz": "^4.7.6",
		"husky": "^4.3.8",
		"lint-staged": "^11.1.2",
		"prettier": "^2.2.1",
		"pug": "^3.0.0",
		"pug-plain-loader": "^1.0.0",
		"sass": "^1.49.9",
		"sass-loader": "^12.6.0",
		"stylelint": "^13.3.2",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-recess-order": "^2.0.4",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-scss": "^3.19.0",
		"typescript": "^4.1.5",
		"vite": "^2.9.6",
		"vite-plugin-eslint": "^1.3.0",
		"vite-plugin-style-import": "^0.9.2",
		"vue-tsc": "^0.34.10"
	},
	"author": "fish uncle",
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"lint-staged": {
		"*.{scss,vue}": "stylelint 'src/**/*.{scss,vue}' --config '.stylelintrc.js' --fix",
		"*.{vue,ts,js}": "eslint --fix",
		"*.{js,ts,css,vue,scss}": "prettier  --plugin-search-dir ./node_modules --config .prettierrc --write"
	}
}
